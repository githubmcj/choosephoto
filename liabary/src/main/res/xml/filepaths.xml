<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!-- external-path:sd ；path:你的应用保存文件的根目录；name随便定义-->
    <!-- root-path 手机存储根目录 -->
    <root-path name="root_path" path="."/>
    <!--Context.getFilesDir().-->
    <files-path path="my_files" name="/myImage"/>
    <!-- getCacheDir().-->
    <cache-path name="my_cache" path="/myImage" />
    <!-- Environment.getExternalStorageDirectory().-->
    <external-path name="my_external" path="/myImage" />
    <!--Context#getExternalFilesDir(String) Context.getExternalFilesDir(null).   -->
    <external-files-path name="my_external_files" path="/myImage" />
    <!-- Context.getExternalCacheDir().  -->
    <external-cache-path name="my_external_cache" path="/myImage" />

    <!-- /storage/emulated/0/Download/${applicationId}/.beta/apk-->
    <external-path name="beta_external_path" path="Download/"/>
    <!--/storage/emulated/0/Android/data/${applicationId}/files/apk/-->
    <external-path name="beta_external_files_path" path="Android/data/"/>
</paths>